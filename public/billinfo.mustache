<div>
    <h2>{{name}}</h2>
    <div style="margin-right: 5%">
        <p>{{summary}}</p>
        <p>{{lastDate}}</p>
        <p>Senate Pass: {{senatePass}} and House Pass: {{housePass}}</p>
        <h3>Action:</h3>
        {{#actions}}
            <div style="margin-right: 5%">
                <p>{{date}}</p>
                <p>{{chamber}}</p>
                <p>{{type}}</p>
                <p>{{description}}</p>
            </div>
        {{/actions}}
        <h3>Votes:</h3>
        {{#vote}}
            <div style="margin-right: 5%">
                <p>{{date}}</p>
                <p>{{chamber}}</p>
                <p>{{question}}</p>
                <p>{{result}}</p>
                <a href="{{link}}">View Graph</a>
            </div>
        {{/vote}}
    </div>
</div>